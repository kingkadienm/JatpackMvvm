<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--选中效果-->
    <item android:state_selected="true">
        <shape android:shape="oval">
            <size android:width="@dimen/dp_11" android:height="@dimen/dp_11" />
            <solid android:color="#F67900" />
        </shape>
    </item>
    <!--默认效果-->
    <item>
        <shape android:shape="oval">
            <size android:width="@dimen/dp_11" android:height="@dimen/dp_11" />
            <stroke android:width="@dimen/dp_0_5" android:color="@color/color_FFFFFF" />
        </shape>
    </item>
</selector>
